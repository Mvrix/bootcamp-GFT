programa
{
	
	funcao inicio()
	{
		inteiro contador,limite,resultado,tabuada

		contador = 0
		limite = 10

		
		//usuario ira inserir o valor da variavel tabuada
		escreva("Qual o valor da Tabuada que deseja exebir? ")
		leia(tabuada)

		faca{
			
			resultado = tabuada * contador
			escreva(tabuada + " X " + contador + " = " + resultado + "\n")
			//++ = +1
			contador ++
			
		}enquanto (contador<=limite)
	}
}
